<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blackbaka.sc.core.dao.mapper.OrdersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.blackbaka.sc.core.dao.model.Orders">
        <id column="id" property="id" />
        <result column="account_id" property="accountId" />
        <result column="stock_id" property="stockId" />
        <result column="count" property="count" />
        <result column="single_money" property="singleMoney" />
        <result column="all_money" property="allMoney" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_id, stock_id, count, single_money, all_money
    </sql>

</mapper>
